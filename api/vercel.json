{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "functions": {
      "api/test.js": {
        "memory": 3009,
        "maxDuration": 30,
        "regions": ["jnb1"]
      },
      "api/*.js": {
        "memory": 3009,
        "maxDuration": 30,
        "regions": ["jnb1"]
      },
      "*.js": {
        "regions": ["jnb1"]
      }
    }
  }
  