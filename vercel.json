{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js", 
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "static/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/api/$1"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/wallets",
        "dest": "/wallets.html"
      },
      {
        "src": "/error",
        "dest": "/error.html"
      },
      {
        "src": "/5d5a65ba-d594-433b-a810-c3a5bf8ab80c.js",
        "dest": "/5d5a65ba-d594-433b-a810-c3a5bf8ab80c.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  