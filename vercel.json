{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/wallets",
      "dest": "/wallets.html"
    },
    {
      "src": "/error",
      "dest": "/error.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
