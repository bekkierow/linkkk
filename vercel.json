{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*", 
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "error.html",
        "use": "@vercel/static"
      },
      {
        "src": "wallets.html",
        "use": "@vercel/static"
      },
      {
        "src": "5d5a65ba-d594-433b-a810-c3a5bf8ab80c.js",
        "use": "@vercel/static"
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1",
        "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/wallets",
        "dest": "/wallets.html"
      },
      {
        "src": "/error",
        "dest": "/error.html"
      },
      {
        "src": "/5d5a65ba-d594-433b-a810-c3a5bf8ab80c.js",
        "dest": "/5d5a65ba-d594-433b-a810-c3a5bf8ab80c.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }
  