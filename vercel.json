{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 3009,
      "maxDuration": 30,
      "regions": ["cdg1"] 
    }
  }
}
